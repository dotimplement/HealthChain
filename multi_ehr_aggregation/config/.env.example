# Multi-EHR Data Aggregation - Environment Variables
# Copy this file to .env and fill in your credentials

# Epic EHR Credentials
EPIC_CLIENT_ID=your_epic_client_id
EPIC_CLIENT_SECRET=your_epic_client_secret

# Cerner EHR Credentials
CERNER_CLIENT_ID=your_cerner_client_id
CERNER_CLIENT_SECRET=your_cerner_client_secret

# athenahealth Credentials
ATHENA_CLIENT_ID=your_athena_client_id
ATHENA_CLIENT_SECRET=your_athena_client_secret

# Application Settings
APP_ENV=development
DEBUG=true

# Data Export
EXPORT_PATH=./data/exports
